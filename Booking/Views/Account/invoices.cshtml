                                                <span class="badge-secondary"><i class="ti ti-point-filled "></i>@Model.status</span>
                                                    }
                                                    else if (Model.status == "Inpogress")
                                                    {
                                                <span class="badge-warning"><i class="ti ti-point-filled "></i>@Model.status</span>
                                                    }
                                                    else if (Model.status == "Cancelled")
                                                    {
                                                <span class="badge-danger"><i class="ti ti-point-filled "></i>@Model.status</span>
                                                    }else{
                                                <span class="badge-success"><i class="ti ti-point-filled "></i>@Model.status</span>
                                                    }





                                          @*   <div>
                                                <img src="~/assets/img/invoice/qr.svg" class="img-fluid" alt="QR">
                                            </div> *@
