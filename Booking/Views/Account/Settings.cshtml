                                <!-- Settings Content -->


                                <script>
                                    document.addEventListener("DOMContentLoaded", function () {
                                        const provinceSelect = document.getElementById("province");
                                        const districtSelect = document.getElementById("district");
                                        const wardSelect = document.getElementById("ward");

                                        // Giả sử bạn đã có giá trị tỉnh, quận, phường từ model (backend)
                                        const selectedProvince = '@Model.Province';  // Giá trị tỉnh đã lưu từ model
                                        const selectedDistrict = '@Model.District';  // Giá trị quận đã lưu từ model
                                        const selectedWard = '@Model.Ward';  // Giá trị phường đã lưu từ model

                                        console.log('Selected Province: ', selectedProvince);
                                        console.log('Selected District: ', selectedDistrict);
                                        console.log('Selected Ward: ', selectedWard);

                                        // Lấy danh sách tỉnh/thành phố
                                        fetch("https://provinces.open-api.vn/api/?depth=1")
                                            .then(response => response.json())
                                            .then(provinces => {
                                                provinces.forEach(province => {
                                                    const option = document.createElement("option");
                                                    option.value = province.code;
                                                    option.textContent = province.name;

                                                    // Nếu tỉnh trong cơ sở dữ liệu khớp với tỉnh trong API, chọn tỉnh đó
                                                    if (province.code == selectedProvince) {
                                                        option.selected = true;
                                                    }

                                                    provinceSelect.appendChild(option);
                                                });

                                                // Nếu đã có giá trị tỉnh được chọn, load danh sách quận
                                                if (selectedProvince) {
                                                    provinceSelect.value = selectedProvince;
                                                    const event = new Event("change");
                                                    provinceSelect.dispatchEvent(event);
                                                }
                                            })
                                            .catch(error => console.error("Error fetching provinces:", error));

                                        // Khi chọn tỉnh/thành phố, lấy danh sách quận/huyện
                                        provinceSelect.addEventListener("change", function () {
                                            const provinceCode = this.value;

                                            // Reset danh sách quận/huyện và phường/xã
                                            districtSelect.innerHTML = '<option value="">Select</option>';
                                            wardSelect.innerHTML = '<option value="">Select</option>';

                                            if (provinceCode) {
                                                fetch(`https://provinces.open-api.vn/api/p/${provinceCode}?depth=2`)
                                                    .then(response => response.json())
                                                    .then(province => {
                                                        if (province.districts) {
                                                            province.districts.forEach(district => {
                                                                const option = document.createElement("option");
                                                                option.value = district.code;
                                                                option.textContent = district.name;

                                                                // Nếu quận trong cơ sở dữ liệu khớp với quận trong API, chọn quận đó
                                                                if (district.code == selectedDistrict) {
                                                                    option.selected = true;
                                                                }

                                                                districtSelect.appendChild(option);
                                                            });

                                                            // Nếu đã có giá trị quận được chọn, load danh sách phường
                                                            if (selectedDistrict) {
                                                                districtSelect.value = selectedDistrict;
                                                                const event = new Event("change");
                                                                districtSelect.dispatchEvent(event);  // Kích hoạt sự kiện change để load phường
                                                            }
                                                        } else {
                                                            console.error("No districts found for this province.");
                                                        }
                                                    })
                                                    .catch(error => console.error("Error fetching districts:", error));
                                            }
                                        });

                                        // Khi chọn quận/huyện, lấy danh sách phường/xã
                                        districtSelect.addEventListener("change", function () {
                                            const districtCode = this.value;

                                            // Reset danh sách phường/xã
                                            wardSelect.innerHTML = '<option value="">Select</option>';

                                            if (districtCode) {
                                                fetch(`https://provinces.open-api.vn/api/d/${districtCode}?depth=2`)
                                                    .then(response => response.json())
                                                    .then(district => {
                                                        if (district.wards) {
                                                            district.wards.forEach(ward => {
                                                                const option = document.createElement("option");
                                                                option.value = ward.code;
                                                                option.textContent = ward.name;

                                                                // Nếu phường trong cơ sở dữ liệu khớp với phường trong API, chọn phường đó
                                                                if (ward.code == selectedWard) {
                                                                    option.selected = true;
                                                                }

                                                                wardSelect.appendChild(option);
                                                            });
                                                        } else {
                                                            console.error("No wards found for this district.");
                                                        }
                                                    })
                                                    .catch(error => console.error("Error fetching wards:", error));
                                            }
                                        });

                                        // Nếu đã có giá trị phường được chọn, gán vào wardSelect
                                        if (selectedWard) {
                                            wardSelect.value = selectedWard;
                                        }
                                    });
                                </script>







