                                                var count = 0;
                                           
                                            foreach(var item in Model)
                                            {
                                                count++;
                                                <tr>
                                                    <td><a href="javascript:void(0);" class="link-primary fw-medium" data-bs-toggle="modal" data-bs-target="#upcoming">@count</a></td>
                                                    <td>
                                                        <div class="d-flex align-items-center">
                                                            <a href="/home/TourDetail/@item.TourID" class="avatar avatar-lg"><img src="@item.img" class="img-fluid rounded-circle" alt="img"></a>
                                                            <div class="ms-2">
                                                                <p class="text-dark mb-0 fw-medium fs-14"><a href="/home/TourDetail/@item.TourID">@item.TourName</a></p>
                                                                <span class="fs-14 fw-normal text-gray-6">@item.Category</span>
                                                            </div>
                                                        </div>
                                                    </td>
                                                    <td>
                                                        @item.Guest
