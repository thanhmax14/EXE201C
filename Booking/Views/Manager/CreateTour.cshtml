    <script>

        document.getElementById('file-input-hotel').addEventListener('change', function (event) {
            const files = event.target.files;
            const hotelGallery = document.getElementById('hotel-gallery');
            hotelGallery.innerHTML = ''; // Xóa các ảnh hiện có trong gallery trước khi thêm mới

            // Duyệt qua tất cả các file đã chọn
            Array.from(files).forEach((file, index) => {
                // Kiểm tra nếu file là hình ảnh
                if (file && file.type.startsWith('image')) {
                    const reader = new FileReader();

                    // Đọc file dưới dạng URL
                    reader.onload = function (e) {
                        const imgElement = document.createElement('a');
                        imgElement.href = 'javascript:void(0);';
                        imgElement.classList.add('gallery-upload-img', 'me-2');
                        imgElement.id = 'hotel-img-' + index; // Đặt ID cho ảnh của khách sạn

                        const imgTag = document.createElement('img');
                        imgTag.src = e.target.result;
                        imgTag.alt = 'Img';

                        const trashIcon = document.createElement('span');
                        trashIcon.classList.add('trash-icon', 'd-flex', 'align-items-center', 'justify-content-center', 'text-danger', 'gallery-trash');

                        const icon = document.createElement('i');
                        icon.classList.add('isax', 'isax-trash');
                        trashIcon.appendChild(icon);

                        imgElement.appendChild(imgTag);
                        imgElement.appendChild(trashIcon);

                        hotelGallery.appendChild(imgElement);
                    };

                    reader.readAsDataURL(file);
                }
            });
        });

    </script>
