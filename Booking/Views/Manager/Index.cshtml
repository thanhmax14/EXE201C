    <script>
        document.addEventListener("DOMContentLoaded", function () {
            fetch('/manager/GetBookingStatistics') // Gọi API từ Controller
                .then(response => response.json())
                .then(data => {
                    if (document.querySelector("#booking-chart")) {
                      
                        if (window.bookingChartInstance) {
                            window.bookingChartInstance.destroy();
                        }

                        var hotelCount = data.hotels;
                        var tourCount = data.tours;

                        document.getElementById("hotel-bookings").innerText = hotelCount;
                        document.getElementById("tour-bookings").innerText = tourCount;

                        var donutChart = {
                            chart: {
                                height: 180,
                                type: 'donut',
                                toolbar: { show: false }
                            },
                            legend: { show: false },
                            colors: ['#212E47', '#98AA30'],
                            series: [hotelCount, tourCount],
                            labels: ['Hotels', 'Tour'],
                            plotOptions: {
                                pie: {
                                    donut: {
                                        size: '65%',
                                    },
                                },
                            },
                            stroke: {
                                width: 3,
                                colors: ['#fff'],
                            },
                            dataLabels: { enabled: false }
                        };

                        window.bookingChartInstance = new ApexCharts(
                            document.querySelector("#booking-chart"),
                            donutChart
                        );

                        window.bookingChartInstance.render();
                    }
                })
                .catch(error => console.error("Error loading booking statistics:", error));
        });
    </script>

    <script>
        fetch('/manager/GetEarningsData')
            .then(response => response.json())
            .then(data => {
                console.log("Earnings Data:", data); // Debug dữ liệu JSON

                // Đảm bảo lấy đúng key
                let hotelRevenue = data.map(item => item.hotelRevenue);
                let tourRevenue = data.map(item => item.tourRevenue);

                // Tính tổng doanh thu cả năm
                let totalEarnings = hotelRevenue.reduce((sum, val) => sum + val, 0) +
                    tourRevenue.reduce((sum, val) => sum + val, 0);

                // Cập nhật số tiền vào HTML
                document.getElementById("total-earnings").textContent = "$" + totalEarnings.toLocaleString();

                var options = {
                    chart: { height: 280, type: 'bar', stacked: true, toolbar: { show: false } },
                    colors: ['#BD1435', '#18BD14'],
                    plotOptions: {
                        bar: { borderRadius: 5, horizontal: false, endingShape: 'rounded' }
                    },
                    series: [
                        { name: 'Hotels', data: hotelRevenue },
                        { name: 'Tours', data: tourRevenue }
                    ],
                    xaxis: {
                        categories: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'],
                        labels: { style: { colors: '#4E5561', fontSize: '12px' } }
                    },
                    yaxis: {
                        labels: {
                            formatter: (val) => val / 1000 + 'K',
                            offsetX: -15,
                            style: { colors: '#4E5561', fontSize: '13px' }
                        }
                    },
                    grid: { show: false },
                    legend: { show: true },
                    fill: { opacity: 1 }
                };

                if (window.earningChartInstance) {
                    window.earningChartInstance.destroy();
                }

                window.earningChartInstance = new ApexCharts(document.querySelector("#earning-chart"), options);
                window.earningChartInstance.render();
            })
            .catch(error => console.error("Error loading earnings data:", error));

    </script>

