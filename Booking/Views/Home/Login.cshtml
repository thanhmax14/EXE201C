<script type="text/javascript">
    $("#btnLogin").on("click", function () {
        $('#btnLogin').html('<i class="fa fa-spinner fa-spin"></i> Đang xử lý...').prop('disabled', true);
        var username = $("#username").val();
        var password = $("#password").val();
        var ReturnUrl = $("#ReturnUrl").val();
        var rememberMe = $("#remembers_me").is(":checked");
        var token = $("input[name='__RequestVerificationToken']").val();
        $.ajax({
            url: '@Url.Action("Login", "Home")',
            method: "POST",
            dataType: "JSON",
            data: {
                username: username,
                password: password,
                ReturnUrl: ReturnUrl,
                rememberMe: rememberMe,
                __RequestVerificationToken: token
            },
            success: function (response) {
                if (response.status == 'success') {
                    cuteToast({
                        type: "success",
                        message: response.msg,
                        timer: 5000
                    });
                    setTimeout("location.href = '" + response.redirectUrl + "';", 100);
                } else if (response.status == 'verify') {
                    Swal.fire({
                        title: "Nhập mã xác minh 2 bước",
                        input: "text",
                        inputAttributes: {
                            autocapitalize: "off"
                        },
                        showCancelButton: true,
                        confirmButtonText: "Xác nhận",
                        showLoaderOnConfirm: true,
                        preConfirm: async (otp) => {
                            if (!otp) {
                                Swal.showValidationMessage("Vui lòng nhập mã xác minh.");
                            }
                            return otp;
                        },
                        allowOutsideClick: () => !Swal.isLoading()
                    }).then(async (result) => {
                        if (result.isConfirmed) {
                            var otp = result.value;

                            $.ajax({
                                url: '@Url.Action("VerifyTwoFactor", "Home")',
                                method: "POST",
                                dataType: "JSON",
                                data: {
                                    username: username,
                                    otp: otp,
                                    __RequestVerificationToken: token,
                                    ReturnUrl: ReturnUrl,

                                },
                                success: function (response) {
                                    if (response.status == 'success') {
                                        cuteToast({
                                            type: "success",
                                            message: response.msg,
                                            timer: 5000
                                        });
                                        setTimeout("location.href = '" + response.redirectUrl + "';", 100);
                                    } else {
                                        Swal.fire('Thất bại', response.msg, 'error');
                                    }
                                },
                                error: function () {
                                    cuteToast({
                                        type: "error",
                                        message: 'Không thể xử lý',
                                        timer: 5000
                                    });
                                }
                            });
                        }
                    });
                } else if (response.status == 'notComfirm') {
                    Swal.fire(
                        'Thất bại',
                        response.msg,
                        'error'
                    );
                } else {
                    Swal.fire(
                        'Thất bại',
                        response.msg,
                        'error'
                    );
                }
                $('#btnLogin').html('Đăng Nhập').prop('disabled', false);
            },
            error: function () {
                cuteToast({
                    type: "error",
                    message: 'Không thể xử lý',
                    timer: 5000
                });
                $('#btnLogin').html('Đăng Nhập').prop('disabled', false);
            }
        });
    });
</script>